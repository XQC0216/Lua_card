                {
                  LinearLayout,
                  layout_width="60dp",
                  layout_height="60dp",
                  layout_marginLeft="0dp";
                  id='lb',
                };
                {
                  LinearLayout,
                  layout_width="-1",
                  layout_height="60dp",
                  layout_marginRight="60dp";
                  id='b',
                };
                {
                  LinearLayout,
                  layout_width="60dp",
                  layout_height="60dp",
                  layout_marginLeft="-60dp";
                  id='rb',
            };
        };
        };
        {
          TextView,
          text="卡片";
          textSize="120sp";
          layout_marginTop="0dp";
