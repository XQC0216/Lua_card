            {
              LinearLayout,
              layout_width="-1",
              layout_height="60dp",
              orientation='horizontal',
                {
                  LinearLayout,
                  layout_width="60dp",
                  layout_height="60dp",
                  layout_marginLeft="0dp";
                  id='lt',
                };
                {
                  LinearLayout,
                  layout_width="-1",
                  layout_height="60dp",
                  layout_marginRight="60dp";
                  id='t',
                };
                {
                  LinearLayout,
                  layout_width="60dp",
                  layout_height="60dp",
